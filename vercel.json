{
    "builds": [{
        "src": "rsic/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "rsic/wsgi.py"
        
    }],
    "rewrites": [
      { "source": "/static/(.*)", "destination": "/staticfiles/$1" },
      { "source": "/(.*)", "destination": "/api/$1" }
    ]
}
